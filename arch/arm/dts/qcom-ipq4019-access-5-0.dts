#include "qcom-ipq4019.dtsi"
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	model = "Bit Access 5.0";
	compatible = "bit,access-5-0";

	aliases {
		led-boot = &led_status;
		led-failsafe = &led_status;
		led-running = &led_status;
		led-upgrade = &led_upgrade;
	};

	soc {
		counter@4a1000 {
			compatible = "qcom,qca-gcnt";
			reg = <0x4a1000 0x4>;
		};

		sfp: sfp {
			compatible = "sff,sfp";
			i2c-bus = <&blsp1_i2c4>;
			tx-fault-gpios  = <&tlmm 0 GPIO_ACTIVE_HIGH>;
			tx-disable-gpios = <&tlmm 1 GPIO_ACTIVE_HIGH>;
			mod-def0-gpios = <&tlmm 2 GPIO_ACTIVE_LOW>;
			rate-select0-gpios = <&tlmm 3 GPIO_ACTIVE_LOW>;
			los-gpios = <&tlmm 4 GPIO_ACTIVE_HIGH>;
			maximum-power-milliwatt = <2000>;
		};
	};

	keys {
		compatible = "gpio-keys";

		reset {
			label = "reset";
			gpios = <&tlmm 8 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};
	};

	leds {
		compatible = "gpio-leds";

		led_status: status {
			label = "green:status";
			gpios = <&tlmm 37 GPIO_ACTIVE_HIGH>;
			panic-indicator;
		};

		led_upgrade: upgrade {
			label = "green:upgrade";
			gpios = <&tlmm 40 GPIO_ACTIVE_HIGH>;
		};

		wlan2g {
			label = "green:wlan2g";
			gpios = <&tlmm 46 GPIO_ACTIVE_HIGH>;
			linux,default-trigger = "phy0tpt";
		};

		wlan5g {
			label = "green:wlan5g";
			gpios = <&tlmm 48 GPIO_ACTIVE_HIGH>;
			linux,default-trigger = "phy1tpt";
		};
	};
};

&prng {
	status = "okay";
};

&crypto {
	status = "okay";
};

&watchdog {
	status = "okay";
};

&vqmmc {
	status = "okay";
};

&sdhci {
	status = "okay";

	pinctrl-0 = <&sd_pins>;
	pinctrl-names = "default";
	cd-gpios = <&tlmm 21 GPIO_ACTIVE_LOW>;
	vqmmc-supply = <&vqmmc>;
	no-1-8-v;
};

&qpic_bam {
	status = "okay";
};

&tlmm {
	mdio_pins: mdio_pinmux {
		mux_1 {
			pins = "gpio6";
			function = "mdio";
			bias-pull-up;
		};

		mux_2 {
			pins = "gpio7";
			function = "mdc";
			bias-pull-up;
		};
	};

	serial_pins: serial_pinmux {
		mux {
			pins = "gpio16", "gpio17";
			function = "blsp_uart0";
			bias-disable;
		};
	};

	spi_0_pins: spi_0_pinmux {
		pinmux {
			function = "blsp_spi0";
			pins = "gpio13", "gpio14", "gpio15";
			drive-strength = <12>;
			bias-disable;
		};

		pinmux_cs {
			function = "gpio";
			pins = "gpio12";
			drive-strength = <2>;
			bias-disable;
			output-high;
		};
	};

	nand_pins: nand_pins {
		pullups {
			pins =  "gpio52", "gpio53", "gpio58", "gpio59";
			function = "qpic";
			bias-pull-up;
		};

		pulldowns {
			pins = "gpio54", "gpio55", "gpio56", "gpio57",
				"gpio60", "gpio62", "gpio63", "gpio64",
				"gpio65", "gpio66", "gpio67", "gpio68",
				"gpio69";
			function = "qpic";
			bias-pull-down;
		};
	};

	sd_pins: sd_pins {
		pinmux {
			function = "sdio";
			pins = "gpio23", "gpio24", "gpio25", "gpio26",
				"gpio28", "gpio29", "gpio30", "gpio31";
			drive-strength = <10>;
		};

		pinmux_sd_clk {
			function = "sdio";
			pins = "gpio27";
			drive-strength = <16>;
		};

		pinmux_sd7 {
			function = "sdio";
			pins = "gpio32";
			drive-strength = <10>;
			bias-disable;
		};
	};

	i2c0_pins: i2c0_pins {
		mux_i2c {
			function = "blsp_i2c0";
			pins = "gpio10", "gpio11";
			drive-strength = <16>;
			bias-disable;
		};
	};

	i2c1_pins: i2c1_pins {
		mux_i2c {
			function = "blsp_i2c1";
			pins = "gpio34", "gpio35";
			drive-strength = <16>;
			bias-disable;
		};
	};
};

&blsp1_i2c3 {
	status = "okay";

	pinctrl-0 = <&i2c0_pins>;
	pinctrl-names = "default";
};

&blsp1_i2c4 {
	status = "okay";

	pinctrl-0 = <&i2c1_pins>;
	pinctrl-names = "default";

	temp-sensor@49 {
		compatible = "ti,tmp1075";
		reg = <0x49>;
	};
};

&blsp_dma {
	status = "okay";
};

&blsp1_spi1 {
	status = "okay";

	pinctrl-0 = <&spi_0_pins>;
	pinctrl-names = "default";
	cs-gpios = <&tlmm 12 GPIO_ACTIVE_HIGH>;

	flash@0 {
		compatible = "jedec,spi-nor";
		spi-max-frequency = <24000000>;
		reg = <0>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "SBL1";
				reg = <0x00000000 0x00040000>;
				read-only;
			};

			partition@40000 {
				label = "MIBIB";
				reg = <0x00040000 0x00020000>;
				read-only;
			};

			partition@60000 {
				label = "QSEE";
				reg = <0x00060000 0x00060000>;
				read-only;
			};

			partition@c0000 {
				label = "CDT";
				reg = <0x000c0000 0x00010000>;
				read-only;
			};

			partition@d0000 {
				label = "DDRPARAMS";
				reg = <0x000d0000 0x00010000>;
				read-only;
			};

			partition@e0000 {
				label = "APPSBLENV"; /* uboot env */
				reg = <0x000e0000 0x00010000>;
			};

			partition@f0000 {
				label = "APPSBL"; /* uboot */
				reg = <0x000f0000 0x00080000>;
			};

			partition@170000 {
				label = "ART";
				reg = <0x00170000 0x00010000>;
				read-only;
				compatible = "nvmem-cells";
				#address-cells = <1>;
				#size-cells = <1>;

				macaddr_art_0: macaddr@0 {
					reg = <0x0 0x6>;
				};

				macaddr_art_6: macaddr@6 {
					reg = <0x6 0x6>;
				};

				precal_art_1000: precal@1000 {
					reg = <0x1000 0x2f20>;
				};

				precal_art_5000: precal@5000 {
					reg = <0x5000 0x2f20>;
				};
			};

			partition@180000 {
				label = "cfg";
				reg = <0x00180000 0x00040000>;
			};

			partition@1c0000 {
				label = "recovery";
				reg = <0x001c0000 0x01e40000>;
			};
		};
	};
};

&nand {
	status = "okay";

	pinctrl-0 = <&nand_pins>;
	pinctrl-names = "default";

	nand@0 {
		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "ubi";
				reg = <0x00000000 0x10000000>;
			};
		};
	};
};

&blsp1_uart1 {
	status = "okay";

	pinctrl-0 = <&serial_pins>;
	pinctrl-names = "default";
};

&cryptobam {
	status = "okay";
};

&mdio {
	status = "okay";

	pinctrl-0 = <&mdio_pins>;
	pinctrl-names = "default";
	reset-gpios = <&tlmm 19 GPIO_ACTIVE_LOW>;
};

&ethphy4 {
	sfp = <&sfp>;
};

&pcie0 {
	status = "okay";

	perst-gpio = <&tlmm 22 GPIO_ACTIVE_LOW>;
	wake-gpio = <&tlmm 33 GPIO_ACTIVE_LOW>;

	/* Free slot for use */
	bridge@0,0 {
		reg = <0x00000000 0 0 0 0>;
		#address-cells = <3>;
		#size-cells = <2>;
		ranges;
	};
};

&wifi0 {
	status = "okay";

	qcom,ath10k-calibration-variant = "8devices-Habanero";
	nvmem-cell-names = "pre-calibration";
	nvmem-cells = <&precal_art_1000>;
};

&wifi1 {
	status = "okay";

	qcom,ath10k-calibration-variant = "8devices-Habanero";
	nvmem-cell-names = "pre-calibration";
	nvmem-cells = <&precal_art_5000>;
};

&usb3_ss_phy {
	status = "okay";
};

&usb3_hs_phy {
	status = "okay";
};

&usb2_hs_phy {
	status = "okay";
};

&usb2 {
	status = "okay";
};

&usb3 {
	status = "okay";
};
